{
  "name": "linepogl/impartial-pipes",
  "description": "A PHP library with partial functions suitable for the pipe operator",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "iterable",
    "pipe",
    "linq",
    "functional"
  ],
  "homepage": "https://github.com/linepogl/impartial-pipes",
  "authors": [
    {
      "name": "Lazare Inepologlou",
      "email": "linepogl@gmail.com"
    }
  ],
  "autoload": {
    "files": [
      "src/Util/pipe.php",
      "src/Util/iterable_count.php",
      "src/Util/iterable_to_iterator.php",
      "src/Util/LazyRewindableIterator.php",

      "src/Combining/p_merge.php",

      "src/Filtering/p_filter.php",
      "src/Filtering/p_filter_out.php",
      "src/Filtering/p_filter_out_nulls.php",
      "src/Filtering/p_skip.php",
      "src/Filtering/p_skip_while.php",
      "src/Filtering/p_take.php",
      "src/Filtering/p_unique.php",
      "src/Filtering/p_while.php",
      "src/Filtering/p_while_not_null.php",

      "src/Mapping/p_flat_map.php",
      "src/Mapping/p_group_by.php",
      "src/Mapping/p_keys.php",
      "src/Mapping/p_map.php",
      "src/Mapping/p_order_by.php",
      "src/Mapping/p_then_by.php",
      "src/Mapping/p_reindex.php",
      "src/Mapping/p_values.php",

      "src/Reducing/p_any.php",
      "src/Reducing/p_all.php",
      "src/Reducing/p_count.php",
      "src/Reducing/p_first.php",
      "src/Reducing/p_first_key.php",
      "src/Reducing/p_first_key_or.php",
      "src/Reducing/p_first_or.php",
      "src/Reducing/p_implode.php",
      "src/Reducing/p_last.php",
      "src/Reducing/p_last_key.php",
      "src/Reducing/p_last_key_or.php",
      "src/Reducing/p_last_or.php",
      "src/Reducing/p_sum.php",
      "src/Reducing/p_to_array.php",

      "src/Tapping/p_narrow.php",
      "src/Tapping/p_not_null.php",
      "src/Tapping/p_tap.php",
      "src/Tapping/p_foreach.php",
      "src/Tapping/p_unfold.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tst/"
    }
  },
  "require": {
    "php": "^8.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.3",
    "phpstan/phpstan": "^2.1",
    "friendsofphp/php-cs-fixer": "^3.85",
    "linepogl/should": "dev-main"
  },
  "scripts": {
    "phpunit": "vendor/bin/phpunit --testsuite=unit",
    "phpunit:coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit --testsuite=unit --coverage-cobertura=var/cache/.phpunit/cobertura.xml && php dev/coverage.php var/cache/.phpunit/cobertura.xml",
    "phpunit:php85": "vendor/bin/phpunit --testsuite=unit85",
    "phpunit:php85:coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit --testsuite=unit85 --coverage-cobertura=var/cache/.phpunit/cobertura.unit85.xml && php dev/coverage.php var/cache/.phpunit/cobertura.unit85.xml",
    "cs:fix": "php-cs-fixer fix --show-progress=dots --verbose ",
    "cs:validate": "php-cs-fixer fix --show-progress=none --dry-run --no-interaction --stop-on-violation",
    "phpstan": "phpstan --memory-limit=4G"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:linepogl/should.git"
    }
  ]
}
