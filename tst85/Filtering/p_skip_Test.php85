<?php // This file is autogenerated

declare(strict_types=1);

namespace Tests\Filtering;

use PHPUnit\Framework\TestCase;

use function ImpartialPipes\p_skip;
use function Should\shouldRepeatedlyIterateLike;

/**
 * @internal
 */
final class p_skip_Test extends TestCase
{
    public function test_p_skip(): void
    {
        []
        |> p_skip(2)
        |> shouldRepeatedlyIterateLike([]);

        []
        |> p_skip(2, preserveKeys: true)
        |> shouldRepeatedlyIterateLike([]);

        ['a' => 1, 'b' => 2, 'c' => 3, 'd' => 4]
        |> p_skip(2)
        |> shouldRepeatedlyIterateLike([3, 4]);

        ['a' => 1, 'b' => 2, 'c' => 3, 'd' => 4]
        |> p_skip(2, preserveKeys: true)
        |> shouldRepeatedlyIterateLike(['c' => 3, 'd' => 4]);

        ['a' => 1, 'b' => 2, 'c' => 3, 'd' => 4]
        |> p_skip(-2)
        |> shouldRepeatedlyIterateLike([1, 2, 3, 4]);

        ['a' => 1, 'b' => 2, 'c' => 3, 'd' => 4]
        |> p_skip(-2, preserveKeys: true)
        |> shouldRepeatedlyIterateLike(['a' => 1, 'b' => 2, 'c' => 3, 'd' => 4]);

    }
}
