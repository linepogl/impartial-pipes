<?php // This file is autogenerated

declare(strict_types=1);

namespace Tests\Filtering;

use PHPUnit\Framework\TestCase;

use function ImpartialPipes\p_take;
use function Should\shouldIterateLike;

/**
 * @internal
 */
final class p_take_Test extends TestCase
{
    public function test_p_take(): void
    {
        []
        |> p_take(2)
        |> shouldIterateLike([], repeatedly: true);

        []
        |> p_take(2, preserveKeys: true)
        |> shouldIterateLike([], repeatedly: true);

        ['a' => 1, 'b' => 2, 'c' => 3, 'd' => 4]
        |> p_take(2)
        |> shouldIterateLike([1, 2], repeatedly: true);

        ['a' => 1, 'b' => 2, 'c' => 3, 'd' => 4]
        |> p_take(2, preserveKeys: true)
        |> shouldIterateLike(['a' => 1, 'b' => 2], repeatedly: true);

        ['a' => 1, 'b' => 2, 'c' => 3, 'd' => 4]
        |> p_take(-2)
        |> shouldIterateLike([], repeatedly: true);

        ['a' => 1, 'b' => 2, 'c' => 3, 'd' => 4]
        |> p_take(-2, preserveKeys: true)
        |> shouldIterateLike([], repeatedly: true);
    }
}
