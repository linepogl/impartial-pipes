<?php // This file is autogenerated

declare(strict_types=1);

namespace Tests\Filtering;

use ImpartialPipes\LazyRewindableIterator;
use PHPUnit\Framework\TestCase;

use function ImpartialPipes\p_unique_keys;
use function Should\shouldIterateLike;

/**
 * @internal
 */
final class p_unique_keys_Test extends TestCase
{
    public function test_p_unique_keys(): void
    {
        []
        |> p_unique_keys()
        |> shouldIterateLike([], repeatedly: true);

        []
        |> p_unique_keys(preserveKeys: true)
        |> shouldIterateLike([], repeatedly: true);

        new LazyRewindableIterator(static function() { yield 'a' => 1; yield 'a' => 2; yield 'b' => 3; yield 'b' => 4; })
        |> p_unique_keys()
        |> shouldIterateLike([1, 3], repeatedly: true);

        new LazyRewindableIterator(static function() { yield 'a' => 1; yield 'a' => 2; yield 'b' => 3; yield 'b' => 4; })
        |> p_unique_keys(preserveKeys: true)
        |> shouldIterateLike(['a' => 1, 'b' => 3], repeatedly: true);

        ['a' => 1, 'b' => 2, 'cc' => 3, 'dd' => 4]
        |> p_unique_keys(static fn (string $k) => strlen($k))
        |> shouldIterateLike([1, 3], repeatedly: true);

        ['a' => 1, 'b' => 2, 'cc' => 3, 'dd' => 4]
        |> p_unique_keys(static fn (string $k) => strlen($k), preserveKeys: true)
        |> shouldIterateLike(['a' => 1, 'cc' => 3], repeatedly: true);
    }
}
