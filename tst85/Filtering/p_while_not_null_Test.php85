/**
 * @internal
 */
final class p_while_not_null_Test extends TestCase
{
    public function test_p_while_not_null(): void
    {
        []
        |> p_while_not_null()
        |> shouldIterateLike([], repeatedly: true);

        []
        |> p_while_not_null(preserveKeys: true)
        |> shouldIterateLike([], repeatedly: true);

        ['a' => 1, 'b' => 2, 'c' => null, 'd' => 4]
        |> p_while_not_null()
        |> shouldIterateLike([1, 2], repeatedly: true);

        ['a' => 1, 'b' => 2, 'c' => null, 'd' => 4]
        |> p_while_not_null(preserveKeys: true)
        |> shouldIterateLike(['a' => 1, 'b' => 2], repeatedly: true);
    }
}
