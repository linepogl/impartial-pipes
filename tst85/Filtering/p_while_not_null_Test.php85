<?php // This file is autogenerated

declare(strict_types=1);

namespace Tests\Filtering;

use PHPUnit\Framework\TestCase;

use function ImpartialPipes\p_while_not_null;
use function Should\shouldRepeatedlyIterateLike;

/**
 * @internal
 */
final class p_while_not_null_Test extends TestCase
{
    public function test_p_while_not_null(): void
    {
        []
        |> p_while_not_null()
        |> shouldRepeatedlyIterateLike([]);

        []
        |> p_while_not_null(preserveKeys: true)
        |> shouldRepeatedlyIterateLike([]);

        ['a' => 1, 'b' => 2, 'c' => null, 'd' => 4]
        |> p_while_not_null()
        |> shouldRepeatedlyIterateLike([1, 2]);

        ['a' => 1, 'b' => 2, 'c' => null, 'd' => 4]
        |> p_while_not_null(preserveKeys: true)
        |> shouldRepeatedlyIterateLike(['a' => 1, 'b' => 2]);
    }
}
