<?php // This file is autogenerated

declare(strict_types=1);

namespace Tests\Filtering;

use PHPUnit\Framework\TestCase;

use function ImpartialPipes\p_filter_out_nulls;
use function Should\shouldRepeatedlyIterateLike;

/**
 * @internal
 */
final class p_filter_out_nulls_Test extends TestCase
{
    public function test_p_filter_out_nulls(): void
    {
        []
        |> p_filter_out_nulls()
        |> shouldRepeatedlyIterateLike([]);

        []
        |> p_filter_out_nulls(preserveKeys: true)
        |> shouldRepeatedlyIterateLike([]);

        ['a' => 1, 'b' => null, 'c' => 3, 'd' => null]
        |> p_filter_out_nulls()
        |> shouldRepeatedlyIterateLike([1, 3]);

        ['a' => 1, 'b' => null, 'c' => 3, 'd' => null]
        |> p_filter_out_nulls(preserveKeys: true)
        |> shouldRepeatedlyIterateLike(['a' => 1, 'c' => 3]);
    }
}
