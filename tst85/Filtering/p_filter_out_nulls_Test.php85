<?php // This file is autogenerated

declare(strict_types=1);

namespace Tests\Filtering;

use Tests\UnitTestCase;

use function ImpartialPipes\p_filter_out_nulls;
use function Tests\p_assert_iterates_like;

/**
 * @internal
 */
final class p_filter_out_nulls_Test extends UnitTestCase
{
    public function test_p_filter_out_nulls(): void
    {
        []
        |> p_filter_out_nulls()
        |> p_assert_iterates_like([]);

        []
        |> p_filter_out_nulls(preserveKeys: true)
        |> p_assert_iterates_like([]);

        ['a' => 1, 'b' => null, 'c' => 3, 'd' => null]
        |> p_filter_out_nulls()
        |> p_assert_iterates_like([1, 3]);

        ['a' => 1, 'b' => null, 'c' => 3, 'd' => null]
        |> p_filter_out_nulls(preserveKeys: true)
        |> p_assert_iterates_like(['a' => 1, 'c' => 3]);
    }
}
