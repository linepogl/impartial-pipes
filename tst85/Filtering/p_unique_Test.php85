<?php // This file is autogenerated

declare(strict_types=1);

namespace Tests\Filtering;

use PHPUnit\Framework\TestCase;

use function ImpartialPipes\p_unique;
use function Should\shouldIterateLike;

/**
 * @internal
 */
final class p_unique_Test extends TestCase
{
    public function test_p_unique(): void
    {
        []
        |> p_unique()
        |> shouldIterateLike([], repeatedly: true);

        []
        |> p_unique(preserveKeys: true)
        |> shouldIterateLike([], repeatedly: true);

        ['a' => 1, 'b' => 2, 'c' => 1, 'd' => 4]
        |> p_unique()
        |> shouldIterateLike([1, 2, 4], repeatedly: true);

        ['a' => 1, 'b' => 2, 'c' => 1, 'd' => 4]
        |> p_unique(preserveKeys: true)
        |> shouldIterateLike(['a' => 1, 'b' => 2, 'd' => 4], repeatedly: true);

        ['a' => 1, 'b' => 2, 'c' => 3, 'd' => 4]
        |> p_unique(static fn (int $x) => $x % 2)
        |> shouldIterateLike([1, 2], repeatedly: true);

        ['a' => 1, 'b' => 2, 'c' => 3, 'd' => 4]
        |> p_unique(static fn (int $x) => $x % 2, preserveKeys: true)
        |> shouldIterateLike(['a' => 1, 'b' => 2], repeatedly: true);

        ['a' => 1, 'aa' => 2, 'b' => 3, 'bb' => 4]
        |> p_unique(static fn (int $x, string $k) => $k[0])
        |> shouldIterateLike([1, 3], repeatedly: true);

        ['a' => 1, 'aa' => 2, 'b' => 3, 'bb' => 4]
        |> p_unique(static fn (int $x, string $k) => $k[0], preserveKeys: true)
        |> shouldIterateLike(['a' => 1, 'b' => 3], repeatedly: true);
    }
}
