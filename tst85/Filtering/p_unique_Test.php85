<?php // This file is autogenerated

declare(strict_types=1);

namespace Tests\Filtering;

use Tests\UnitTestCase;

use function ImpartialPipes\p_unique;
use function Tests\p_assert_iterates_like;

/**
 * @internal
 */
final class p_unique_Test extends UnitTestCase
{
    public function test_p_unique(): void
    {
        []
        |> p_unique()
        |> p_assert_iterates_like([]);

        []
        |> p_unique(preserveKeys: true)
        |> p_assert_iterates_like([]);

        ['a' => 1, 'b' => 2, 'c' => 1, 'd' => 4]
        |> p_unique()
        |> p_assert_iterates_like([1, 2, 4]);

        ['a' => 1, 'b' => 2, 'c' => 1, 'd' => 4]
        |> p_unique(preserveKeys: true)
        |> p_assert_iterates_like(['a' => 1, 'b' => 2, 'd' => 4]);

        ['a' => 1, 'b' => 2, 'c' => 3, 'd' => 4]
        |> p_unique(static fn (int $x) => $x % 2)
        |> p_assert_iterates_like([1, 2]);

        ['a' => 1, 'b' => 2, 'c' => 3, 'd' => 4]
        |> p_unique(static fn (int $x) => $x % 2, preserveKeys: true)
        |> p_assert_iterates_like(['a' => 1, 'b' => 2]);

        ['a' => 1, 'aa' => 2, 'b' => 3, 'bb' => 4]
        |> p_unique(static fn (int $x, string $k) => $k[0])
        |> p_assert_iterates_like([1, 3]);

        ['a' => 1, 'aa' => 2, 'b' => 3, 'bb' => 4]
        |> p_unique(static fn (int $x, string $k) => $k[0], preserveKeys: true)
        |> p_assert_iterates_like(['a' => 1, 'b' => 3]);
    }
}
