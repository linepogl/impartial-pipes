<?php // This file is autogenerated

declare(strict_types=1);

namespace Tests\Filtering;

use PHPUnit\Framework\TestCase;
use PHPUnitMetaConstraints\Util\PHPUnitMetaConstraintsTrait;

use function ImpartialPipes\p_unique;

/**
 * @internal
 */
final class p_unique_Test extends TestCase
{
    use PHPUnitMetaConstraintsTrait;

    public function test_p_unique(): void
    {
        []
        |> p_unique()
        |> self::iteratesLike([], rewind: true);

        []
        |> p_unique(preserveKeys: true)
        |> self::iteratesLike([], rewind: true);

        ['a' => 1, 'b' => 2, 'c' => 1, 'd' => 4]
        |> p_unique()
        |> self::iteratesLike([1, 2, 4], rewind: true);

        ['a' => 1, 'b' => 2, 'c' => 1, 'd' => 4]
        |> p_unique(preserveKeys: true)
        |> self::iteratesLike(['a' => 1, 'b' => 2, 'd' => 4], rewind: true);

        ['a' => 1, 'b' => 2, 'c' => 3, 'd' => 4]
        |> p_unique(static fn (int $x) => $x % 2)
        |> self::iteratesLike([1, 2], rewind: true);

        ['a' => 1, 'b' => 2, 'c' => 3, 'd' => 4]
        |> p_unique(static fn (int $x) => $x % 2, preserveKeys: true)
        |> self::iteratesLike(['a' => 1, 'b' => 2], rewind: true);

        ['a' => 1, 'aa' => 2, 'b' => 3, 'bb' => 4]
        |> p_unique(static fn (int $x, string $k) => $k[0])
        |> self::iteratesLike([1, 3], rewind: true);

        ['a' => 1, 'aa' => 2, 'b' => 3, 'bb' => 4]
        |> p_unique(static fn (int $x, string $k) => $k[0], preserveKeys: true)
        |> self::iteratesLike(['a' => 1, 'b' => 3], rewind: true);
    }
}
