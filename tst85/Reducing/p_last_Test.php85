<?php // This file is autogenerated

declare(strict_types=1);

namespace Tests\Reducing;

use ArrayIterator;
use OutOfBoundsException;
use PHPUnit\Framework\TestCase;

use function ImpartialPipes\p_last;
use function Tests\p_assert_equals;
use function Tests\p_assert_throws;

/**
 * @internal
 */
final class p_last_Test extends TestCase
{
    public function test_p_last_with_arrays(): void
    {
        static fn () => (
            []
            |> p_last()
        ) |> p_assert_throws(OutOfBoundsException::class);

        static fn () => (
            []
            |> p_last(static fn (int $x) => $x < 3)
        ) |> p_assert_throws(OutOfBoundsException::class);

        [1,2,3]
        |> p_last()
        |> p_assert_equals(3);

        [1,2,3]
        |> p_last(static fn (int $x) => $x < 3)
        |> p_assert_equals(2);

        static fn () => (
            [1,2,3]
            |> p_last(static fn (int $x) => $x > 3)
        ) |> p_assert_throws(OutOfBoundsException::class);

        ['a' => 1, 'aa' => 2, 'aaa' => 3]
        |> p_last()
        |> p_assert_equals(3);

        ['a' => 1, 'aa' => 2, 'aaa' => 3]
        |> p_last(static fn (int $x, string $k) => strlen($k) < 3)
        |> p_assert_equals(2);

        static fn () => (
            ['a' => 1, 'aa' => 2, 'aaa' => 3]
            |> p_last(static fn (int $x, string $k) => strlen($k) > 3)
        ) |> p_assert_throws(OutOfBoundsException::class);
    }

    public function test_p_last_with_iterables(): void
    {
        static fn () => (
            new ArrayIterator([])
            |> p_last()
        ) |> p_assert_throws(OutOfBoundsException::class);

        static fn () => (
            new ArrayIterator([])
            |> p_last(static fn (int $x) => $x < 3)
        ) |> p_assert_throws(OutOfBoundsException::class);

        new ArrayIterator([1,2,3])
        |> p_last()
        |> p_assert_equals(3);

        new ArrayIterator([1,2,3])
        |> p_last(static fn (int $x) => $x < 3)
        |> p_assert_equals(2);

        static fn () => (
            new ArrayIterator([1,2,3])
            |> p_last(static fn (int $x) => $x > 3)
        ) |> p_assert_throws(OutOfBoundsException::class);

        new ArrayIterator(['a' => 1, 'aa' => 2, 'aaa' => 3])
        |> p_last()
        |> p_assert_equals(3);

        new ArrayIterator(['a' => 1, 'aa' => 2, 'aaa' => 3])
        |> p_last(static fn (int $x, string $k) => strlen($k) < 3)
        |> p_assert_equals(2);

        static fn () => (
            new ArrayIterator(['a' => 1, 'aa' => 2, 'aaa' => 3])
            |> p_last(static fn (int $x, string $k) => strlen($k) > 3)
        ) |> p_assert_throws(OutOfBoundsException::class);
    }
}
