<?php // This file is autogenerated

declare(strict_types=1);

namespace Tests\Reducing;

use Tests\SimpleIterator;
use Tests\UniterableArrayIterator;
use PHPUnit\Framework\TestCase;

use function ImpartialPipes\p_to_array;
use function Tests\p_assert_equals;

/**
 * @internal
 */
final class p_to_array_Test extends TestCase
{
    public function test_p_to_array(): void
    {
        []
        |> p_to_array()
        |> p_assert_equals([]);

        [1,2]
        |> p_to_array()
        |> p_assert_equals([1,2]);

        new SimpleIterator([1,2,3])
        |> p_to_array()
        |> p_assert_equals([1,2,3]);

        new UniterableArrayIterator([1,2,3,4])
        |> p_to_array()
        |> p_assert_equals([1,2,3,4]);
    }
}
