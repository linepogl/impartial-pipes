<?php // This file is autogenerated

declare(strict_types=1);

namespace Tests\Mapping;

use PHPUnit\Framework\TestCase;

use function ImpartialPipes\p_flat_map;
use function Should\shouldIterateLike;

/**
 * @internal
 */
final class p_flat_map_Test extends TestCase
{
    public function test_p_flat_map(): void
    {
        []
        |> p_flat_map(fn (int $x) => [$x, $x])
        |> shouldIterateLike([], repeatedly: true);

        ['a' => 1, 'b' => 2, 'c' => 3, 'd' => 4]
        |> p_flat_map(fn (int $x) => [$x, $x])
        |> shouldIterateLike([1, 1, 2, 2, 3, 3, 4, 4], repeatedly: true);

        ['a' => 1, 'b' => 2, 'c' => 3, 'd' => 4]
        |> p_flat_map(fn (int $x, string $k) => [$x, $k])
        |> shouldIterateLike([1, 'a', 2, 'b', 3, 'c', 4, 'd'], repeatedly: true);
    }
}
