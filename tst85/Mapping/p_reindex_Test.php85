<?php // This file is autogenerated

declare(strict_types=1);

namespace Tests\Mapping;

use Tests\UnitTestCase;

use function ImpartialPipes\p_reindex;
use function Tests\p_assert_iterates_like;

/**
 * @internal
 */
final class p_reindex_Test extends UnitTestCase
{
    public function test_p_reindex(): void
    {
        []
        |> p_reindex(fn (int $x) => $x * $x)
        |> p_assert_iterates_like([]);

        [1, 2, 3, 4]
        |> p_reindex(fn (int $x) => $x * $x)
        |> p_assert_iterates_like([1 => 1, 4 => 2, 9 => 3, 16 => 4]);

        ['a' => 1, 'b' => 2, 'c' => 3, 'd' => 4]
        |> p_reindex(fn (int $x) => $x * $x)
        |> p_assert_iterates_like([1 => 1, 4 => 2, 9 => 3, 16 => 4]);

        ['a' => 1, 'b' => 2, 'c' => 3, 'd' => 4]
        |> p_reindex(fn (int $x, string $k) => $x . $k)
        |> p_assert_iterates_like(['1a' => 1, '2b' => 2, '3c' => 3, '4d' => 4]);

        ['a' => 1, 'b' => 1, 'c' => 2, 'd' => 2]
        |> p_reindex(fn (int $x) => $x * $x)
        |> p_assert_iterates_like([1 => 1, 4 => 2]);
    }
}
