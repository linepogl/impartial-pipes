<?php // This file is autogenerated

declare(strict_types=1);

namespace Tests\Combining;

use PHPUnit\Framework\TestCase;

use function ImpartialPipes\p_merge;
use function Should\shouldIterateLike;

/**
 * @internal
 */
final class p_merge_Test extends TestCase
{
    public function test_p_merge(): void
    {
        []
        |> p_merge([])
        |> shouldIterateLike([], repeatedly: true);

        []
        |> p_merge([], preserveKeys: true)
        |> shouldIterateLike([], repeatedly: true);

        [1,2]
        |> p_merge([3,4])
        |> shouldIterateLike([1,2,3,4], repeatedly: true);

        [1,2]
        |> p_merge([3,4], preserveKeys: true)
        |> shouldIterateLike([1,2], repeatedly: true);

        ['a' => 1,'b' => 2,'c' => 3]
        |> p_merge(['b' => 22,'c' => 33,'d' => 4])
        |> shouldIterateLike([1,2,3,22,33,4], repeatedly: true);

        ['a' => 1,'b' => 2,'c' => '3']
        |> p_merge(['b' => 2,'c' => 3,'d' => 4], preserveKeys: true)
        |> shouldIterateLike(['a' => 1,'b' => 2,'c' => '3','d' => 4], repeatedly: true);
    }
}
