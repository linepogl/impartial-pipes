<?php // This file is autogenerated

declare(strict_types=1);

namespace Tests\Util;

use ArrayIterator;
use IteratorAggregate;
use Override;
use Tests\UniterableArrayIterator;
use PHPUnit\Framework\TestCase;
use Traversable;

use function ImpartialPipes\iterable_to_iterator;
use function Tests\p_assert_instance_of;

/**
 * @internal
 */
final class iterable_to_iterator_Test extends TestCase
{
    public function test_iterable_to_iterator(): void
    {
        iterable_to_iterator([1, 2, 3])
        |> p_assert_instance_of(ArrayIterator::class);

        iterable_to_iterator(new UniterableArrayIterator([1, 2, 3]))
        |> p_assert_instance_of(UniterableArrayIterator::class);

        iterable_to_iterator(
            new class () implements IteratorAggregate {
                #[Override]
                public function getIterator(): Traversable
                {
                    return new UniterableArrayIterator([1, 2, 3]);
                }
            },
        ) |> p_assert_instance_of(UniterableArrayIterator::class);
    }
}
